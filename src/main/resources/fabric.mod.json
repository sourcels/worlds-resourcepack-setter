	"authors": [
		"shimaper"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/worlds-resourcepack-setter",
		"sources": "https://github.com/sourcels/worlds-resourcepack-setter",
        "issues": "https://github.com/sourcels/worlds-resourcepack-setter/issues"
	},
	"license": "MIT",
	"icon": "assets/worlds-resourcepack-setter/icon.png",
	"entrypoints": {
		"client": [
			"com.shimaper.worldsresourcepacksetter.WorldsResourcepackSetterClient"
		],
